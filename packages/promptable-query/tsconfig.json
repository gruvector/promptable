{
    "compilerOptions": {
        "target": "es2017",
        "lib": [
            "dom",
            "dom.iterable",
            "esnext",
        ],
        "module": "esnext",
        "esModuleInterop": true,
        "forceConsistentCasingInFileNames": true,
        "isolatedModules": true,
        "moduleResolution": "node",
        "preserveWatchOutput": true,
        "skipLibCheck": true,
        "noEmit": true,
        "strict": true,
        "baseUrl": ".",
        "paths": {},
    },
    "exclude": [
        "node_modules"
    ]
}